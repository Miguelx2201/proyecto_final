<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="352.0" prefWidth="235.0" spacing="10" stylesheets="@app.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegistroController">
   <children>
       <Label text="Registro de Cliente" />
       <TextField fx:id="txtNombre" promptText="Nombre" />
       <TextField fx:id="txtCedula" promptText="Cédula" />
       <TextField fx:id="txtDireccion" promptText="Dirección" />
       <TextField fx:id="txtCorreo" promptText="Correo" />
       <TextField fx:id="txtEdad" promptText="Edad" />
       <Button onAction="#handleRegistro" text="Registrar" />
       <Label fx:id="lblError" style="-fx-text-fill:red;" />
   </children>
</VBox>
