<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="226.0" prefWidth="280.0" spacing="10" stylesheets="@app.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BeneficiosController">
   <children>
       <Label text="Puntos y Beneficios" />
       <Label fx:id="lblPuntos" />
       <Label fx:id="lblRango" />
       <HBox spacing="10">
          <children>
              <TextField fx:id="txtCanje" promptText="Monto a canjear" />
              <Button onAction="#canjear" text="Canjear" />
          </children>
       </HBox>
       <Label fx:id="lblMensaje" style="-fx-text-fill:green;" />
       <Button onAction="#volver" text="Volver" />
   </children>
</VBox>
